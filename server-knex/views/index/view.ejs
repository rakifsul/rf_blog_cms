<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="jumbotron jumbotron-lower">
            <h1 class="display-4">
                <%= data.result.title %>
            </h1>
            <% data.resultTags.forEach(function(item){ %>
                <a class="badge bg-primary" href="/tags/<%= item %>">
                    <%= item %>
                </a>
            <% }) %>
            <hr>
            <p class="text-muted">
                <%= data.result.name %> - <%= data.moment().format('DD MMMM, YYYY', data.result.createdAt); %>
            </p>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="ql-editor">
            <%- data.result.content %>
        </div>
    </div>
</div>